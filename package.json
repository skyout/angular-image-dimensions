{
  "name": "angular-image-dimensions",
  "version": "1.1.0",
  "description": "Angular Directive for getting and display displaying image dimensions in the view",
  "main": "dist/angular-image-dimensions.js",
  "homepage": "https://github.com/skyout/angular-image-dimensions#readme",
  "scripts": {
    "build": "coffee --bare --output dist/ --compile src/",
    "build-demo-js": "coffee --bare --output demo/js --compile src/",
    "build-demo-css": "stylus --compress demo/stylus/styles.styl --out demo/css/",
    "watch-coffee": "coffee --bare --watch --output dist/ --compile src/",
    "watch-stylus": "stylus --watch --compress demo/stylus/styles.styl --out demo/css/"
  },
  "engines": {
    "npm": "2.14.20"
  },
  "keywords": [
    "plugin",
    "angular",
    "directive",
    "image",
    "dimensions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyout/angular-image-dimensions.git"
  },
  "author": {
    "name": "Scott Lanning",
    "email": "skyout@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skyout/angular-image-dimensions/issues"
  },
  "dependencies": {
    "angular": "^1.4.9"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "stylus": "^0.54.2"
  }
}
